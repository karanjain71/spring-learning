package com.springcore.springcore2.lifecycleInterface;

public class Samosa {
	private Double price;

	public Samosa() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Double getPrice() {
		return price;
	}

	public void setPrice(Double price) {
		System.out.println("Setting the property in the setter method");
		this.price = price;
	}

	@Override
	public String toString() {
		return "Samosa [price=" + price + "]";
	}
	
	public void init() {
		System.out.println("Instantiating a bean with the init method");
	}
	
	public void destroy1() {
		System.out.println("Destroy a bean with the destroy method");
	}
	
}
