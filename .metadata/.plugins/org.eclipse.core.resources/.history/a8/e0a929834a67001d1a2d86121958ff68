package com.springcore.springcore2.stereotype;

import org.springframework.context.ApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;

public class Test {
	public static void main(String[] args) {
		ApplicationContext context = new ClassPathXmlApplicationContext("com/springcore/springcore2/stereotype/stereotypeConfig.xml");
		Student student = (Student)context.getBean("student", Student.class);
		Student student2 = (Student)context.getBean("student", Student.class);
		Student student3 = (Student)context.getBean("student", Student.class);
		System.out.println(student);
		System.out.println(student2);
		System.out.println(student3);
	}
}
