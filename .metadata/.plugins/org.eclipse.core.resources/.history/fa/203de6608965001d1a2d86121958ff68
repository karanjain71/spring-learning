package com.springcore.springcore2.lifecycle;

import org.springframework.context.support.AbstractApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;

public class Test {
	public static void main(String[] args) {
		AbstractApplicationContext context = new ClassPathXmlApplicationContext("com/springcore/springcore2/lifecycleInterface/config.xml");
		Samosa s1 = (Samosa) context.getBean("samosa1");
		System.out.println(s1);
		//registering shutdown hook here, remember to change the class name to AbstractApplicationContext
		context.registerShutdownHook();
	}
}
